<%= form_with url: search_path, method: :get, local: true do |f| %>
  <%= f.text_field :content %>
  <%= f.select :model, [["User", "user"],["Book","book"]] %>
  <%= f.select :method, [["完全一致", "match"],["前方一致", "forward"], ["後方一致", "backward"], ["部分一致", "partial"]] %>
  <%= f.submit :search %>
<% end %>